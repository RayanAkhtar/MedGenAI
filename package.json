{
  "name": "medgen-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "flask-dev": "cross-env FLASK_DEBUG=1 pip install -r backend/requirements.txt && python -m flask --app backend/main.py run -p 5328",
    "next-dev": "next dev --turbopack",
    "dev": "concurrently -k -n \"NEXT,FLASK\" -c \"blue,green\" \"npm run next-dev\" \"npm run flask-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "next": "15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
